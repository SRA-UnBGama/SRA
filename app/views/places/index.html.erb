<h1>Lista de Locais de Atendimento</h1>

<table>
  <tr>
    <th>Nome</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= link_to 'Editar', edit_place_path(place) %></td>
    <td><%= link_to 'Excluir', place, method: :delete, data: { confirm: 'Deseja confirmar?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo local', new_place_path %>
