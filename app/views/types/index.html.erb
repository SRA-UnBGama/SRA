<div class="container">
  <div class="row">
<div class="page-header">
<h1>Tipos de Atendimentos</h1>
<div align="right" >

<%=link_to raw("<i class=\"icon-white icon-plus\"> Criar Tipo de Atendimento </i>"), 
          new_type_path, 
          class: "btn btn-success  btn-small"%>
</div>
</div>



<table>
  <tr>
    <th>Nome</th>
    <th>Local</th>
    <th></th>

  </tr>

<% @types.each do |type| %>
  <tr>
    <td><%= type.try(:name) %></td>

    
    <td><%= link_to raw("<i class=\"icon-white icon-pencil\"> Editar </i>"), 
          edit_type_path(type), 
          class: "btn btn-small btn-primary",
          method: :get %></td>
    <td><%= link_to raw("<i class=\"icon-white icon-trash\"> Deletar </i>"), 
          type, 
          class: "btn btn-small btn-danger",
          method: :delete, data: { confirm: 'Tem certeza que deseja excluir?'} %></td></td>
  </tr>

<% end %>
</table>

<br />



</div>
</div>