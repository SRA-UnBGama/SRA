<h1>Lista de Tipos de Atendimentos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Local</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @types.each do |type| %>
  <tr>
    <td><%= type.name %></td>
    <td><%= type.place %></td>
    <td><%= link_to 'Mostrar', type %></td>
    <td><%= link_to 'Editar', edit_type_path(type) %></td>
    <td><%= link_to 'Excluir', type, method: :delete, data: { confirm: 'Deseja confirmar?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo tipo', new_type_path %>
